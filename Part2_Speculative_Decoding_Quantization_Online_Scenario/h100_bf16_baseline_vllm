# VLLM Server
VLLM_ATTENTION_BACKEND=FLASH_ATTN vllm serve "Qwen/Qwen3-8B" --max-model-len 8192 --tensor-parallel-size 1 --gpu-memory-utilization 0.9 --seed 42 &> serve_baseline.log &

# Guidellm Client (Prefill-heavy)
guidellm benchmark --target http://localhost:8000 --model "Qwen/Qwen3-8B" \
--data 'prompt_tokens=2048,output_tokens=128' \
--data-samples 300 \
--rate-type sweep \
--warmup 0.01 \
--cooldown 0.01

