# VLLM Server with long context window (FP8 example)
VLLM_USE_FLASHINFER_MOE_FP8=1 VLLM_ATTENTION_BACKEND=FLASHINFER vllm serve "Qwen/Qwen3-8B-FP8" \
--kv-cache-dtype fp8 --max-model-len 32768 --tensor-parallel-size 1 \
--gpu-memory-utilization 0.9 --seed 42 &> serve.log &

# LM-Eval Client (MMLU-Pro example)
HF_ALLOW_CODE_EVAL=1 lm_eval --model local-completions \
--tasks mmlu_pro --batch_size 1 \
--model_args model=Qwen/Qwen3-8B-FP8,base_url=http://localhost:8000/v1/completions,max_length=32768,num_concurrent=64 \
--seed 42 --show_config --confirm_run_unsafe_code --log_samples \
--output_path ./Qwen3-8B_fp8.json --num_fewshot 5 --fewshot_as_multiturn \
--apply_chat_template --gen_kwargs temperature=0.6,top_p=0.95,top_k=20,max_gen_toks=16384 \
--limit 40 &> mmlu_fp8.log &

